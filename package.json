{
  "name": "stackoverflow-lite",
  "version": "1.0.0",
  "description": "StackOverflow-lite is a platform where people can ask questions and provide answers.",
  "main": "index.js",
  "scripts": {
    "test": "mocha --compilers js:babel-core/register",
    "start:dev": "nodemon --watch server --exec babel-node server.js",
    "start:test": "mocha --reporter=min --no-colors --require babel-core/register --require babel-polyfill index.js",
    "cover": "istanbul cover node_modules/mocha/bin/_mocha server/test",
    "coveralls": "npm run cover -- --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shaolinmkz/StackOverflow-lite.git"
  },
  "author": "Nwabuzor_Chukwuemeka_Obiora",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/shaolinmkz/StackOverflow-lite/issues"
  },
  "homepage": "https://github.com/shaolinmkz/StackOverflow-lite#readme",
  "dependencies": {
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.2",
    "express": "^4.16.3",
    "istanbul": "^0.4.5",
    "json-server": "^0.14.0",
    "mocha": "^5.2.0",
    "node-uuid": "^1.4.8",
    "nodemon": "^1.18.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-airbnb": "^2.5.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "morgan": "^1.9.0",
    "supertest": "^3.1.0",
    "babel-core": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0"
  }
}
